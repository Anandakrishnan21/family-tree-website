          <p>Add Mother</p>
          <div class="closeButton1">
            <div class="close-btn1" id="closeBtn1">&times;</div>
          </div>
          <div class="form-inputs">
            <form action="/tree" method="POST">
              <div class="button-div1">
                <span class="male">Male</span>
                <span class="female activate">Female</span>
                <span class="other">Other</span>
              </div>
              <div class="form-groups">
                <label for="fname">First Name</label>
                <input type="text" placeholder="First name" name="motherFname" class="motherFname" />
              </div>
              <div class="form-groups">
                <label for="lname">Last name</label>
                <input type="text" placeholder="Last name" name="motherLname" class="motherLname"/>
              </div>
              <div class="form-groups">
                <label for="name">Living Status</label>
                <div class="button-div1">
                  <span class="living activate">Living</span>
                  <span class="descent">Descent</span>
                  <span class="unknown">Unknown</span>
                </div>
              </div>
              <div class="form-groups">
                <label for="birth">Birth Year </label>
                <input type="date" placeholder="1980" name="motherBirthday" class="motherBirthday"/>
              </div>
              <div class="form-groups">
                <label for="address">Address</label>
                <input type="text" placeholder="Address" class="motherAddress" name="motherAddress"/>
              </div>
              <div class="form-groups">
                <label for="location">Location</label>
                <input type="text" placeholder="location" name="motherLocation" class="motherLocation"/>
              </div>
              <button class="add-to-tree">Add to Tree</button>
